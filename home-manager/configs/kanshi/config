profile {
    output HDMI-A-2 mode 1680x1050 position 0,0
    output DP-1 mode 3840x2160 position 1680,0 scale 1.35
    output eDP-1 disable
}

profile {
    output eDP-1 mode 1920x1080 position 0,0
    output DP-1 mode 3840x2160 position 1920,0 scale 1.35
}

profile {
    output eDP-1 mode 1920x1080 position 0,0
    output HDMI-A-2 mode 1680x1050 position 1920,0
}
